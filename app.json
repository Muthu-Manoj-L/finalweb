{
  "expo": {
    "name": "MyApp",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/images/deepspectrum-logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/deepspectrum-logo.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2ba3a18c-392c-4722-8802-e7d27d9610a5"
      }
    },
    "android": {
      "package": "com.muthu_manoj_l.myexpoapp"
    }
  }
}
